{
  "name": "azure-devops-google-cloud-tools",
  "version": "1.0.0",
  "description": "Google Cloud tasks for Azure DevOps Pipelines, like install CLI and deploy to App Services",
  "main": "index.js",
  "scripts": {
    "test": "mocha test --reporter mocha-junit-reporter",
    "create": "tfx extension create --manifest-globs vss-extension.json",
    "createv": "tfx extension create --manifest-globs vss-extension.json --rev-version",
    "share": "tfx extension publish --manifest-globs vss-extension.json --rev-version --share-with nexsobr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexsolab/azure-devops-google-cloud-tools.git"
  },
  "keywords": [
    "azure",
    "devops",
    "pipelines",
    "release",
    "gcloud",
    "cli",
    "google",
    "cloud",
    "platform",
    "gcp",
    "deploy",
    "task",
    "iac"
  ],
  "author": {
    "name": "Gabriel Anderson",
    "url": "https://github.com/gcacars"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/nexsolab/azure-devops-google-cloud-tools/issues"
  },
  "homepage": "https://github.com/nexsolab/azure-devops-google-cloud-tools#readme",
  "dependencies": {
    "vss-web-extension-sdk": "^5.141.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "sync-request": "^6.1.0"
  }
}
